{
  "name": "vlm-dcl",
  "version": "1.0.0-alpha.60",
  "description": "Virtual Land Manager for Decentraland SDK7",
  "scripts": {
    "start": "sdk-commands start",
    "dev": "tsc -p tsconfig.json --watch",
    "build": "sdk-commands build",
    "upgrade-sdk": "npm install --save-dev @dcl/sdk@latest",
    "upgrade-sdk:next": "npm install --save-dev @dcl/sdk@next"
  },
  "devDependencies": {
    "@dcl/asset-packs": "latest",
    "@dcl/js-runtime": "7.5.7",
    "@dcl/sdk": "latest"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=6.0.0"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 120,
    "trailingComma": "none"
  },
  "dependencies": {
    "colyseus.js": "^0.15.26",
    "whatwg-url-without-unicode": "^8.0.0-3",
    "xmlhttprequest-polyfill": "^1.0.1"
  },
  "bundleDependencies": [
    "colyseus.js",
    "whatwg-url-without-unicode",
    "xmlhttprequest-polyfill"
  ]
}
